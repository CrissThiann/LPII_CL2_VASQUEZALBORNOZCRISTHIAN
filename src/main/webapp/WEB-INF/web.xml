<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ControladorServlet</servlet-name>
  	<display-name>ControladorServlet</display-name>
  	<description></description>
  	<servlet-class>Controladores.ControladorServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletProducto</servlet-name>
  	<display-name>ServletProducto</display-name>
  	<description></description>
  	<servlet-class>Controladores.ServletProducto</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ControladorServlet</servlet-name>
  	<url-pattern>/ControladorServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletProducto</servlet-name>
  	<url-pattern>/ServletProducto</url-pattern>
  </servlet-mapping>
</web-app>
